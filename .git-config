[commit]
	gpgsign = true
[credential]
	helper = osxkeychain
[filter "lfs"]
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	required = true
	smudge = git-lfs smudge -- %f
[gpg]
	program = gpg
[merge]
	tool = bc
[mergetool "bc"]
	trustExitCode = true
[pull]
	rebase = false
[push]
    autoSetupRemote = true
	default = current
[tag]
	forceSignAnnotated = true
